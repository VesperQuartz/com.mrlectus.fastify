{
	"type": "module",
	"name": "com.mrlectus.fastify",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"build:ts": "tsc",
		"watch:ts": "tsc -w",
		"db:push": "drizzle-kit push",
		"test": "bun test",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"auth:generate": "bunx @better-auth/cli generate --output ./src/models/schemas/auth.schema.ts",
		"auth:migrate": "bunx @better-auth/cli migrate",
		"dev": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 bun --watch run server.ts",
		"start": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 bun --bun run server.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^6.0.0",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.0.1",
		"@fastify/csrf-protection": "^7.1.0",
		"@fastify/env": "^5.0.2",
		"@fastify/helmet": "^13.0.1",
		"@fastify/jwt": "^9.1.0",
		"@fastify/multipart": "^9.0.3",
		"@fastify/one-line-logger": "^2.0.2",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/redis": "^7.0.2",
		"@fastify/sensible": "^6.0.0",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@fastify/websocket": "^11.2.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"better-auth": "^1.2.12",
		"drizzle-orm": "^0.44.3",
		"fastify": "^5.0.0",
		"fastify-cli": "^7.4.0",
		"fastify-plugin": "^5.0.0",
		"fastify-type-provider-zod": "^5.0.2",
		"pg": "^8.16.3",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@types/bun": "^1.2.18",
		"@types/node": "^24.0.14",
		"@types/pg": "^8.15.4",
		"c8": "^10.1.2",
		"concurrently": "^9.0.0",
		"drizzle-kit": "^0.31.4",
		"fastify-tsconfig": "^3.0.0",
		"ts-node": "^10.4.0",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.20.3",
		"typescript": "~5.8.2"
	}
}
