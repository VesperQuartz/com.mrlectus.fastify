{
	"type": "module",
	"name": "com.mrlectus.fastify",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"build:ts": "tsc",
		"watch:ts": "tsc -w",
		"db:push": "drizzle-kit push",
		"test": "bun test",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"auth:generate": "bunx @better-auth/cli generate --output ./src/models/schemas/auth.schema.ts",
		"auth:migrate": "bunx @better-auth/cli migrate",
		"dev": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 bun --watch run server.ts",
		"start": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 bun --bun run server.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^6.3.1",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.2.0",
		"@fastify/csrf-protection": "^7.1.0",
		"@fastify/env": "^5.0.3",
		"@fastify/helmet": "^13.0.2",
		"@fastify/jwt": "^10.0.0",
		"@fastify/multipart": "^9.4.0",
		"@fastify/one-line-logger": "^2.1.0",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/redis": "^7.2.0",
		"@fastify/sensible": "^6.0.4",
		"@fastify/swagger": "^9.7.0",
		"@fastify/swagger-ui": "^5.2.5",
		"@fastify/websocket": "^11.2.0",
		"@paralleldrive/cuid2": "^3.3.0",
		"better-auth": "^1.4.18",
		"drizzle-orm": "^0.45.1",
		"fastify": "^5.7.4",
		"fastify-cli": "^7.4.1",
		"fastify-plugin": "^5.1.0",
		"fastify-type-provider-zod": "^6.1.0",
		"pg": "^8.18.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9",
		"@types/node": "^25.3.0",
		"@types/pg": "^8.16.0",
		"c8": "^10.1.3",
		"concurrently": "^9.2.1",
		"drizzle-kit": "^0.31.9",
		"fastify-tsconfig": "^3.0.0",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.21.0",
		"typescript": "~5.9.3"
	}
}
